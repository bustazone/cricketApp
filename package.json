{
  "name": "cricket_app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:clean": "npm start -- --reset-cache",
    "test": "jest",
    "build": "npm i && cd ios/ && pod install",
    "deep-clean": "rm -rf node_modules && npm i && cd ios/ && rm -rf pods && pod install",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx --quiet",
    "lint-fix": "npx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "npx prettier --check \"{src,__tests__}/**/*.{js,jsx,ts,tsx}\"",
    "prettier-fix": "npx prettier --write \"{src,__tests__}/**/*.{js,jsx,ts,tsx}\"",
    "tsc": "npx tsc -p ."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.14.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.9.3",
    "@react-navigation/stack": "^5.14.3",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "2.0.0",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.18.1",
    "react-native-svg": "^12.1.0",
    "react-native-tab-view": "^2.16.0",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/runtime": "^7.13.8",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/node": "^14.0.27",
    "@types/react": "^17.0.0",
    "@types/react-native": "^0.63.44",
    "@types/react-redux": "^7.1.16",
    "@types/redux-logger": "^3.0.8",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.21.0",
    "jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "metro-react-native-babel-preset": "^0.65.1",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "react-test-renderer": "16.14.0",
    "typescript": "^4.1.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
